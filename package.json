{
  "name": "nestjs-gql-pg-poc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "NODE_ENV=staging nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "NODE_ENV=local nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "linter-fix": "pnpm run lint --fix && prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "new-migration": "docker-compose run dbmate new --",
    "apply-migrations": "docker-compose run dbmate"
  },
  "dependencies": {
    "@mercadoni/elementals": "^0.8.0-alpha.5",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/graphql": "^12.0.7",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/schedule": "^3.0.1",
    "@open-syk/common": "^0.4.0",
    "@scaleleap/pg-format": "^1.0.0",
    "@supabase/supabase-js": "^2.26.0",
    "axios": "^1.4.0",
    "class-validator": "^0.14.0",
    "firebase-admin": "^11.9.0",
    "graphql-type-json": "^0.3.2",
    "moment-timezone": "^0.5.43",
    "node-cache": "^5.1.2",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^13.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.9.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "29.6.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 120,
    "useTabs": false
  }
}
